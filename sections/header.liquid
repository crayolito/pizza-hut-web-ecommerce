{% comment %}
  width: 100%;
  height: 92px;
{% endcomment %}
<sticky-header>
  <header
    style="
      position: sticky;
      top: 0;
      background-color: #fff;
      z-index: 1000;
      padding: 1rem;
      box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
      margin: 10px;
      border-radius: 18px;
      display: flex;
      height: 92px;
      padding: 48px;
      justify-content: space-between;
      align-items: center;
      flex-shrink: 0;
      border-radius: 8px;
      border: 1px solid#DEDEDE;
      background: var(--White, #FFF);
    "
  ></header>
</sticky-header>

<script>
  class HeaderPrincipal extends HTMLElement {}

  customElements.define('sticky-header', HeaderPrincipal);
</script>

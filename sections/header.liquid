<sticky-header>
  <header
    style="
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      z-index: 1000;
      padding: 0;
    "
  >
    <div
      style="
        display: flex;
        height: 78px;
        justify-content: space-between;
        align-items: center;
        border-radius: 8px;
        border: 1px solid #DEDEDE;
        background: #FFF;
        margin: 40px 59px 0 59px;
      "
    ></div>
  </header>
</sticky-header>

<script>
  class HeaderPrincipal extends HTMLElement {}

  customElements.define('sticky-header', HeaderPrincipal);
</script>
